{
  "name": "@nestjs-event-driven/repository",
  "private": true,
  "version": "0.0.0",
  "description": "Starter for building npm package",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "start:dev": "tsc --watch",
    "test:dev": "vitest",
    "test": "vitest run",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "build": "tsup",
    "release": "changeset publish",
    "test:e2e": "vitest --config ./vitest.config.e2e.mts"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add -p"
    ],
    "**/*.{ts,tsx}": "tsc-files --noEmit"
  },
  "keywords": [],
  "author": "Andrii Artemenko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artandrey/nest-event-driven.git"
  },
  "bugs": {
    "url": "https://github.com/artandrey/nest-event-driven/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.22.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.8",
    "@nestjs/testing": "^11.0.11",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tsc-files": "^1.1.4",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.1",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.8.2"
  }
}
